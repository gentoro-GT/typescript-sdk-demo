{
  "name": "typescript-sdk-demo",
  "version": "0.0.1",
  "description": "Demo Application using Gentoro SDK for Typescript",
  "keywords": [
    "typescript",
    "sdk",
    "api",
    "client"
  ],
  "author": "Gentoro R&D",
  "homepage": "https://github.com/gentoro-GT/typescript-sdk-demo",
  "bugs": "https://github.com/gentoro-GT/typescript-sdk-demo/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/gentoro-GT/typescript-sdk-demo"
  },
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "start-local": " npm run build:css && vite",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "preview": "vite preview",
    "tailwind:dev": "npx tailwindcss -i ./src/globals.css -o ./src/assets/globals.css --watch",
    "build:css": "postcss ./src/globals.css -o ./src/assets/globals.css"
  },
  "dependencies": {
    "@gentoro/sdk": "0.0.1",
    "@preline/collapse": "^2.6.0",
    "autoprefixer": "^10.4.20",
    "openai": "^4.76.3",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "preline": "^2.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "rehype-external-links": "^3.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.15.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^20",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "react-router-dom": "^7.0.2",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  }
}
